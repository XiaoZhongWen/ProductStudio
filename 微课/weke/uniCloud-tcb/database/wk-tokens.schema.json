// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": ["platform", "token"],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"platform":{
			"bsonType": "string",
			"description": "平台名称"
		},
		"token": {
			"bsonType": "string",
			"description": "平台token"
		}
	}
}