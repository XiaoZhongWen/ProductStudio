// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": ["type", "charge"],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"type": {
			"bsonType": "int",
			"description": "类型, 0:季度会员、1:半年会员、2:一年会员"
		},
		"charge": {
			"bsonType": "double",
			"description": "会员订阅金额"
		}
	}
}